 $(document).ready(function(){
	 
	 applayFormfunction();	 
 
		function applayFormfunction(){
			
			var insurnceId='1556359274299';
			var secondParentId='1553269916267';
			var thirdParentId='1553269913436';
			var fourthPrentId='1553269959717';  
			var fastagid='1556361298928';
			var currentId='1556369874418'; 
			if(thirdParentId==insurnceId || secondParentId==insurnceId ||  fourthPrentId==insurnceId){ //1556359274299     
				
			 $("#applynowform").hide();     

			 $("#upperRow").append('<button type="button" class="startButton" id="startform">Apply Now</button>');

			 $("#startform").on("click",function(){
			 $("#applynowform").show();
			 $("#startform").hide();
			 })
			}    
// production urls https://fastag.aubank.in/AUFastTagApplication/fastTagApplication
// UAT urls https://fastaguat.aubank.in/AUFastTagApplication/fastTagApplication
			if(fastagid==currentId){
				 $("#upperRow").append('<button onclick="window.location.href = \'https://fastag.aubank.in/AUFastTagApplication/fastTagApplication\';" type="button" class="startButton" id="startform">Apply Now</button>');
			}
		}
		 
	

	 }); 
 
 
 
 

  
 
  
