<script type="78ff6dbf9fb963a458e46d21-text/javascript">
$(document).ready(function(){
	var minlength = 3;  
	var searchRequest = null; 
	$("#search").keyup(function() {
		$("#searchBarRes ul").html('');  
		var lsearch=$(this).val().trim();
		 if(lsearch.length>=3){  
		 var ajaxurl="/page-search?search="+lsearch; 
		 $.ajax({        
			url: ajaxurl,  	         
			type: "GET",
			contentType: 'text/plain', 
			error: function(resp)
			{ 
				console.log("ERROR : "+resp); 
			},	     										 
			success: function(resp)
			{     
				$("#searchBarRes ul").html(''); 
				$("#headsearch ul").html('');
				var product = $(resp).find('#lsearc').html(); 
				 
				$(".searchsuggestion").html('');
				$("#searchsuggestion").html(product); 
				console.log($("#searchsuggestion > h1").text()+"Testing>>>>>>>>>>>>>"); 
				if($("#searchsuggestion > h1").text()==""){
					$("div #searchsuggestion .col-sm-12" ).each(function( i ) { 
	 					console.log($(this).find("h3").text());
	 					console.log($(this).find("a").attr("href"));
	 				$("#searchBarRes ul").append("<li><a href="+$(this).find("a").attr("href")+">" + $(this).find("h3").text() + "</a></li>")
	 				$("#headsearch ul").append("<li><a href="+$(this).find("a").attr("href")+">" + $(this).find("h3").text() + "</a></li>")
					});
				}else{
					$("#searchsuggestion > h1").text();
					$("#searchBarRes ul").append("<li><a  style='color:#FF0000'>"+$("#searchsuggestion > h1").text()+ "</a></li>");
					$("#headsearch ul").append("<li><a  style='color:#FF0000'>"+$("#searchsuggestion > h1").text()+ "</a></li>") 
				}    
				$("#searchBarRes").show();    
			}        
					});    
		 }
	  });
	  
})
 
</script>


