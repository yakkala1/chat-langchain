       	    		     $("#City-error").html('Please select City');
       	    		     $("#City-error").css("display", "block"); 
        				}
        			}
        			
        		/* added for DataLayer start */
        		  var ret = document.getElementById('fmtitle1').getAttribute('value').replace(' to get started','');

        			function successdataLayer(){
        				
        				dataLayer.push({
        					'formurl': '/vpv/form/fullname',
        					'formEvent': 'Form Full Name',
        					'formproduct': ret 
        				})
        				dataLayer.push({ 
        					'formurl': '/vpv/form/mobilenumber',
        					'formEvent': 'Form Mobile Number',
        					'formproduct': ret 
        				})
        				dataLayer.push({
        					'formurl': '/vpv/form/email',
        					'formEvent': 'Form Email',
        					'formproduct': ret 
        				})
        				dataLayer.push({
        					'formurl': '/vpv/form/captcha',
        					'formEvent': 'Form Captcha',
        					'formproduct': ret 
        				})
        				dataLayer.push({
        					'formurl': '/vpv/form/applynow',
        					'formEvent': 'Form Apply Now Button',
        					'formproduct': ret 
        				})
        			}
        			
        			function dataLayerfullnameerror(){
        				
        				dataLayer.push({
        					'formurl': '/vpv/form/fullnameerror',
        					'formEvent': 'Form Full Name Error',
        					'formproduct': ret 
        				})
        			}
