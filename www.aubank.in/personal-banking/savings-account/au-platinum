        			function city(val){
        				var cityregex = /^[a-zA-Z]+$/;  
        				if(val!=""){
        					  
        					if(val.length<=2){
    	    						 $("#City-error").html('Minimum 3 Character is expected.');
    	    						$("#City-error").css("display", "block"); 
