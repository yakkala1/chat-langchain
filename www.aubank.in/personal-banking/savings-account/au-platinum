<script type="78ff6dbf9fb963a458e46d21-text/javascript">
   
   $(document).ready(function(){  
		 tmp = []; 
		 var items = location.search.substr(1);
		 console.log(items); 
		 
		  var temp=items.split("&");
		  for (var index = 0; index < temp.length; index++) {
		     tmp = temp[index].split("=");

		     if (tmp[0] === "utm_source"){ ;
		     setCookie("utmparameater",items,10) ; 
		     }  
		 break;
		 
		  }   
		  
		    
		  
		 
	 })  
	   
	  function setCookie(cname, cvalue, exdays) {
		  var d = new Date();
		  console.log("cookie time   "+(exdays*60*1000));  
		  d.setTime(d.getTime() + (exdays*60*1000));
		   
		  var expires = "expires="+ d.toUTCString();
		  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
		}  
   
   </script>


<script type="78ff6dbf9fb963a458e46d21-text/javascript">  
        
      var utmcookie="";   
		var value = "; " + document.cookie;
var parts = value.split("; " + "utmparameater" + "=");
//console.log("parts      "+parts);   
if (parts.length == 2) {utmcookie=parts.pop().split(";").shift();}
//console.log("from comman head   utmcookie"+utmcookie+"Aferttext");   
///console.log("length  "+utmcookie.length);   
if(utmcookie.length!=0){
var url = window.location.href;    
//console.log("url "+url);    
///console.log("url index   "+url.indexOf('?'));    
if (url.indexOf('?') > -1){
  
}else{ 
 url += ('?'+utmcookie);     
}   
window.history.replaceState(null, null, url);
}else{
	//console.log("utmcookie else");  
} 
      </script>


