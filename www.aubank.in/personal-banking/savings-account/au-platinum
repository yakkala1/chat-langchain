                  $("#applynow").click(function(){
    				let errorfields = {};
    				var flag=false;
    				$.each($(".error"),function(){
    				if ($(this).is(':visible')) {
    				   console.log("find=="+$(this).attr("id"));
    				   errorfields[$(this).parent().find(".label").text().trim()]=$(this).attr("id").trim();
    				   flag=true;
    				}else{
    				console.log("Not find=="+$(this).attr("id"))
    				 errorfields[$(this).parent().find(".label").text().trim()]="success";
    				}
    				});
    				if(flag){
    				var notproductform = document.getElementsByClassName('selectText');
    					var dataLayerProduct= "";
    					if (notproductform.length == 0) {
    					  var productcode=$("#formcode").text().trim();
    					 console.log(productcode.substring(productcode.indexOf('(')+1,productcode.length-1));
    					 dataLayerProduct = productcode.substring(productcode.indexOf('(')+1,productcode.length-1);
    					}else{
    						dataLayerProduct = errorfields["Interested Product"];
    					}
    				
    					dataLayer.push({
    				'event': 'FormFailure',
    				'Full_Name': errorfields["Full Name"],
    				'Mobile_Number': errorfields["Mobile Number"],
    				'Email': errorfields["Email"],
    				'Product': dataLayerProduct,
    				'form_city':errorfields["City"],
    				'Captcha': errorfields["Captcha"]
    				  })  
    				}else{
    					  //let productData = $('.selectText option:selected').text();
    					 /*dataLayer.push({ 
    						'event': 'FormSuccess',
    						'Product':productData
    						});	*/ 
    				  }
    				console.log("errorfields JSON="+errorfields["Full Name"]);
    				})
        			/* added for DataLayer end */	 
    /* 4-April-2020 added dataLayer product code*/ 	 
    /* 8-April-2020 added cities API & remove UTM statics parts & added city dataLayer*/ 	   
    /* 14-May-2020 CDT script addition && Ajay Custom code is added at the top */ 
    /* 20-May-2020 duplicate leads button disabled */ 
    /* 14-Jan-2021 NRI form call added */	
     /* 26-March-2021 adobe script added */	
    /* 8-JULY-2021 NRI World condition  added */	
    			    
        		});       
   </script>
