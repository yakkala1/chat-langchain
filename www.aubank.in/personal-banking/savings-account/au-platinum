     	    				      datalast=JSON.stringify(jsonData);            
     	    				       
        				  	    $.ajax({     
        				  		url: ajaxurl,	         
        				  		type: "POST",      
        				  		crossDomain:true,
        				  		data: datalast, 
        				  		contentType: 'application/json', 
        				  		error: function(resp)
        				  		{ 
        				  			$('#Load').delay(1000).fadeOut('slow');
        				  		},	    										 
        				  		success: function(resp)
        				  		{  
        				  			 var productData="";
        				  			$('#Load').delay(1000).fadeOut('slow');
    								var notproductform = document.getElementsByClassName('selectText');
    								if (notproductform.length == 0) {
    								  var productcode=$("#formcode").text().trim();
    								 console.log(productcode.substring(productcode.indexOf('(')+1,productcode.length-1));
    								 productData=productcode.substring(productcode.indexOf('(')+1,productcode.length-1);
    								}else{
        						  productData = $('.selectText option:selected').text();
    								}
        				  			var respData = JSON.parse(resp);
        				  			/* {"cmassage":"Successfull","saveflag":"Successfull","rflag":"Successfull","cflag":"Successfull"} */
        				  			if(respData.cflag =="Successfull" && respData.dbflag=="Successfull" && respData.rflag=="Successfull" && respData.saveflag=="Successfull" )
        				  			//if(respData.cflag =="Successfull" && respData.rflag=="Successfull" && respData.saveflag=="Successfull" )
        				  			{
   									digitalData.eventName="Lead Submit";           
   									 _satellite.track("event"); 
        	        	 				 
        				  				if(formId=='customerfeedback'){
        				  					window.location.href ='/thank-you-for-feedback';
        	    				        }else{
