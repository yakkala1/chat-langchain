	$(document).ready(function() {	
	$( ".widget p" ).click(
		function() {
		$('.widget p').not(this).parent().removeClass('active');
			$(this).parent().toggleClass('active'); 
			
		}
	);
	getMobileDetails();             
});  
	     
	function getMobileDetails() {
		var userAgent = navigator.userAgent || navigator.vendor || window.opera;
		if (/windows phone/i.test(userAgent)) {
			console.log("Windows Phone");
			$("#footerAppList").empty();
			$("#footerAppList").append("<a href='https://apps.apple.com/in/app/au-mobile/id1532869687' target='_blank'><img src='/assets/images/AppStore_white.svg' alt='App Icon' style='height: 25px;' ><span class='tx1'>App Store</span>  &nbsp; &nbsp; </a>");
			 
		}
		if (/android/i.test(userAgent)) {
			console.log("Android");
			$("#footerAppList").empty();
			$("#footerAppList").append("<a href='https://play.google.com/store/apps/details?id=com.ausmallfinancebank.amb' target='_blank'><img src='/assets/images/PlayStore_white.svg' alt='playStore' style='height: 25px;'> <span class='tx1'>Play Store</span></a> ");
		}
		if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
			console.log("iOS");
			$("#footerAppList").empty(); 

			$("#footerAppList").append("<a href='https://apps.apple.com/in/app/au-mobile/id1532869687' target='_blank'><img src='/assets/images/AppStore_white.svg' alt='App Icon' style='height: 25px;' ><span class='tx1'>App Store</span>  &nbsp; &nbsp; </a>");
			
		}

		console.log("Desktop......");      
		  
		}
