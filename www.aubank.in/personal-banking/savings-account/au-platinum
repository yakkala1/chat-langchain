        				if(val!=""){
        					if(textArea.test(val)){
        	    		      }else{ 
        	    		    	  $("#Remark-error").html("Enter Valid data");
        	    		    	  $("#Remark-error").show();
        	    		      }
        				}
        				else{
        					$("#Remark-error").show(); 
        				}
        			}
