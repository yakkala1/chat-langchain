        									'event': 'CaptchaFailure',
        									'Product':productData 
        									}); 
        				  				var ajaxurl="/auform/captcha";
        				  				 $.ajax({     
        				  					url: ajaxurl, 	     
        				  					type: "POST",
        				  					async: false,
        				  					contentType: 'text/plain', 
        				  					error: function(resp)
        				  					{
        				  						console.log("ERROR : "+resp); 
        				  					},	    										 
        				  					success: function(resp)
        				  					{  
        									$(".captcha").empty();
        				  						$(".captcha").append(resp);
        				  					} 
        				  							}); 
        				  				
        				  				for (var key in respData) {
        								    if (respData.hasOwnProperty(key)) {
        								      var val = respData[key];
        								    }
        								  }
        				  				 if(respData.cflag =="Failed")
        				  				 { 
        				  					    $("input[name='Captcha_captcha_true']").val(''); 
        					  					$("#Captcha-error").html('');
        										$("#Captcha-error").append(respData.cmassage);
        						  				$("#Captcha-error").css("display", "block"); 
        					  				} 
        				  				if(respData.dbflag =="Failed")
        				  				 {    $("#Feild-error").html('');
        				  					    $("#Feild-error").append(respData.dbmassage);
        						  				$("#Feild-error").css("display", "block");   
