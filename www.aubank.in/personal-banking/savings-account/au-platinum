            				  				$("form").html('');
            				  				$(".fmtitle").html('');
        	    				        	window.location.href ='/thankyou?pcode='+formcode;	
        	    				        }
        				  				  dataLayer.push({
        									'event': 'FormSuccess',
        									'Product':productData
        									});	
    										
    										/*cdt script addition*/
        				  				console.log("CDT fullName="+jsonData.Fullname_fulltext_true);
        				  			console.log("CDT mobile="+jsonData.MobileNumber_mobile_true);
        				  			console.log("CDT email="+jsonData.Email_email_true);
        				  			console.log("CDT city="+jsonData.City_text_true); 
        				  			console.log("CDT interested="+productData);  
        				  				var contactData = {
    										'pk^mobile':jsonData.MobileNumber_mobile_true,
    										'email':jsonData.Email_email_true,
    										'FULLNAME':jsonData.Fullname_fulltext_true,
    										'CITY':jsonData.City_text_true,
    										'PRODUCTNAME':productData
    										};
    										
    										var dispatchData = {
    										'mobile':jsonData.MobileNumber_mobile_true,
    										'email':jsonData.Email_email_true,
    										'fullname':jsonData.Fullname_fulltext_true,
    										'city':jsonData.City_text_true,
    										'productname':productData
    										};   
    										smartech('contact', '817', contactData, function() {
    										smartech('identify', jsonData.MobileNumber_mobile_true);
    										smartech('dispatch', 'Website_Leads', dispatchData);
    										});
        				  				/*cdt script addition ENDS*/
        				  			}else{ 
    								   $("#applynow").prop('disabled', false); 
        								//dataLayerfullnameerror();
        								//dataLayermobilenumbererror();
        								//dataLayeremailerror();
        								//dataLayercaptchaerror();
        								//dataLayerapplynowerror();
