<script type="78ff6dbf9fb963a458e46d21-text/javascript">  
 $(document).ready(function () {
		$("img.tpsrch").click(function () {
			$("#topSearchBar").css("display", "block");
		});

		$(".search >img.srch").click(function () {
			
			var searchparm = $("#Searchbox").val().trim();
			if (searchparm != "" && searchparm != null) {
				$("#banner_Search").submit();
			}
		});
		 
		$(".searchbar >img.srch").click(function(){
			var searchparm = $("#search").val().trim();
			
			if (searchparm != "" && searchparm != null) {
 				$("#header_Search").submit();
			}
		})

		$(document).on('keypress', function (e) {
			if ($(".search input[name='search']").is(":focus") && e.key == "Enter") {
				var searchparm = $(".search input[name='search']").val().trim();
				if (searchparm != "" && searchparm != null) {
					$("#banner_Search").submit(); 
				}
			}
			if ($("#search").is(":focus") && e.key == "Enter") {
				var searchparm = $("#search").val().trim();
				if (searchparm != "" && searchparm != null) {
					$("#header_Search").submit();  
				}
			}
		});
	});

	function startDictation() {
		console.log("Hello"); 
		if (window.hasOwnProperty('webkitSpeechRecognition')) {
			var recognition = new webkitSpeechRecognition();
			var recognizing;
			recognition.continuous = false;
			recognition.interimResults = false;
			recognition.lang = "en-US";
			recognition.start();
			recognition.onresult = function (e) {
				if ($("#Searchbox").length) {
					document.getElementById('Searchbox').value = e.results[0][0].transcript;
				}
   
				$('#speak-text').text(e.results[0][0].transcript);
				$('#search').val(e.results[0][0].transcript);
				var searchparm = e.results[0][0].transcript.trim();
				if (searchparm != "" && searchparm != null) {
					
					$("#banner_Search").submit();
				}
				setTimeout(function () {
					$('#myModal').modal('hide');
				}, 1000);
				recognition.stop();
			};
			recognition.onend = function () {
			}
			recognition.onerror = function (e) {
				console.log("error "+e.error);
				console.log(e); 	 
				recognition.stop(); 
			}
		}
	}

</script>

